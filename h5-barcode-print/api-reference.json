            },
            "operator": {
              "type": "string",
              "required": true,
              "description": "操作者ID"
            }
          },
          "response": "返回结果«条码信息»"
        },
        "scanbtcode": {
          "path": "/pda/scanbtcode",
          "method": "GET",
          "summary": "扫本体码得到内包装码打印信息",
          "params": {
            "btcode": {
