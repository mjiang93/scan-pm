  "apis": {
    "barcode": {
      "name": "PC条码管理相关接口",
      "endpoints": {
        "list": {
          "path": "/pc/page",
          "method": "POST",
          "summary": "查询条码列表",
          "requestBody": {
            "page": {
              "type": "integer",
              "description": "起始页"
            },
            "size": {
              "type": "integer",
              "description": "分页大小"
            },
            "deliveryDateStart": {
              "type": "string",
              "format": "date-time",
              "description": "送货日期开始"
            },
            "deliveryDateEnd": {
              "type": "string",
              "format": "date-time",
              "description": "送货日期结束"
            },
            "offset": {
              "type": "integer",
              "description": "偏移量"
            },
            "traceId": {
              "type": "string",
              "description": "事件号"
            }
          },
          "response": "返回结果«分页对象«条码信息»»"
        },
        "detail": {
          "path": "/pc/detail",
          "method": "GET",
          "summary": "根据ID查询条码详情",
          "params": {
            "id": {
              "type": "string",
              "required": true,
              "description": "主键id"
            }
          },
          "response": "返回结果«条码信息»"
        },
        "edit": {
          "path": "/pc/edit",
          "method": "POST",
          "summary": "修改条码信息",
          "requestBody": "条码信息对象",
          "response": "返回结果«object»"
        },
        "btprint": {
          "path": "/pc/btprint",
          "method": "POST",
          "summary": "本体码打印预览-生成条码-更新图纸版本后才能打印",
          "params": {
            "id": {
              "type": "string",
              "required": true,
              "description": "主键"
            },
            "operator": {
              "type": "string",
              "required": true,
              "description": "操作者ID"
            }
          },
          "response": "返回结果«object»"
        },
        "editprint": {
          "path": "/pc/editprint",
          "method": "POST",
          "summary": "打印更新打印状态-点打印按扭时调用",
          "requestBody": {
            "id": {
              "type": "integer",
              "format": "int64",
              "required": true,
              "description": "主键"
            },
            "operator": {
              "type": "string",
              "required": true,
              "description": "操作人"
            },
            "btPrintCnt": {
              "type": "integer",
              "format": "int32",
              "required": true,
              "description": "本体码打印状态 1-打印"
            },
            "nbzPrintCnt": {
              "type": "integer",
              "format": "int32",
              "required": true,
              "description": "内包装码打印状态 1-打印"
            },
            "wbzPrintCnt": {
              "type": "integer",
              "format": "int32",
              "required": true,
              "description": "送货外包装码打印 1-打印"
            }
          },
          "response": "返回结果«object»"
        },
        "editdelivery": {
          "path": "/pc/editdelivery",
          "method": "POST",
          "summary": "批量更新送货日期",
          "requestBody": {
            "ids": {
              "type": "array",
              "items": {
                "type": "integer",
                "format": "int64"
              },
              "required": true,
              "description": "主键集合"
            },
            "deliveryDate": {
              "type": "string",
              "format": "date-time",
              "required": true,
              "description": "送货日期"
            },
            "operator": {
              "type": "string",
              "required": true,
              "description": "操作人"
            }
          },
          "response": "返回结果«object»"
        },
        "editaccessory": {
          "path": "/pc/editaccessory",
          "method": "POST",
          "summary": "批量更新附件",
          "requestBody": {
            "ids": {
              "type": "array",
              "items": {
                "type": "integer",
                "format": "int64"
              },
              "required": true,
              "description": "主键集合"
            },
            "accessoryCnt": {
              "type": "integer",
              "format": "int32",
              "required": true,
              "description": "附件数量"
            },
            "operator": {
              "type": "string",
              "required": true,
              "description": "操作人"
            }
          },
          "response": "返回结果«object»"
        }
      }
    },
